{
        "manifest_version": 3,
        "name": "Solar Link",
        "version": "1.0.0",
        "description": "Solar Link Node",
        
        "icons": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        },

        "permissions": ["storage", "offscreen"],
        "action": {
            "default_icon": "icons/icon-128.png",
            "default_popup": "index.html"
        }
    }
    